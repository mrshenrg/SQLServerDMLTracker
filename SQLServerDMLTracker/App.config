<?xml version="1.0"?>
<configuration>

  <connectionStrings>
    <!--填入数据库文件的路径即可，这里我们放在debug目录下，所有只需要直接写入文件名MHS_DB.db-->
    <add name="ConnStr" connectionString="Server=localhost;Initial Catalog=master;Integrated Security=True"/>
  </connectionStrings>
  <appSettings>
    <add key="ConnStr" value="Server=localhost;Initial Catalog=master;Integrated Security=True"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
